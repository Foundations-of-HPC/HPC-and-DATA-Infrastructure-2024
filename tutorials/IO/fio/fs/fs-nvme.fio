[global]
filename=/mnt/mydisk/fiotest
ioengine=libaio
direct=1
buffered=0
invalidate=1
time_based=0
runtime=20
size=40G
group_reporting=1
stonewall=1
loops=1
iodepth=4

[Write]
rw=write
bs=32m
numjobs=1

[Read]
rw=read
bs=32m
numjobs=1

[Write_2]
rw=write
bs=32m
numjobs=2

[Read_2]
rw=read
bs=32m
numjobs=2
