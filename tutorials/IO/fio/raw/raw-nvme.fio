[global]
filename=/dev/nvme1n1
ioengine=libaio
direct=1
buffered=0
invalidate=1
time_based=0
runtime=20
size=16G
group_reporting=1
stonewall=1
loops=1
iodepth=8

[Write_nvme]
rw=write
bs=8m
numjobs=1

[Read_nvme]
rw=read
bs=8m
numjobs=1


